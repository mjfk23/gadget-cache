{
  "name": "mjfk23/gadget-cache",
  "description": "Gadget library for working with cache",
  "type": "library",
  "keywords": [],
  "homepage": "https://github.com/mjfk23/gadget-cache",
  "license": "MIT",
  "authors": [
    {
      "name": "Melody Kimball",
      "role": "lead"
    }
  ],
  "support": {
    "issues": "https://github.com/mjfk23/gadget-cache/issues",
    "wiki": "https://github.com/mjfk23/gadget-cache/wiki",
    "source": "https://github.com/mjfk23/gadget-cache"
  },
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "mjfk23/gadget-core": "^3.0.0-alpha",
    "mjfk23/symfony-bundle": "^1.0",
    "psr/cache": "^3.0"
  },
  "require-dev": {
    "mjfk23/gadget-code-style": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Gadget\\Cache\\": ["src/", "bundle/"]
    }
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "apcu-autoloader": true
  },
  "scripts": {
    "phpcs": "vendor/bin/phpcs",
    "phpstan": "vendor/bin/phpstan analyse -c phpstan.neon"
  }
}
